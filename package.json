{
  "name": "dudulu-blog",
  "private": true,
  "description": "A blog developing with Gatsby",
  "version": "0.1.0",
  "author": "dudulu <fadedspreadsea@gmail.com>",
  "dependencies": {
    "@sentry/browser": "^5.6.1",
    "@storybook/react": "^6.0.20",
    "autoprefixer": "^9.5.1",
    "gatsby": "^2.3.16",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-no-sourcemaps": "^2.1.1",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "sass": "^1.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^6.0.20",
    "@storybook/addon-essentials": "^6.0.20",
    "@storybook/addon-links": "^6.0.20",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "react-is": "^16.13.1"
  },
  "keywords": [
    "dudulu",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": " rm -rf .cache && npm run develop",
    "serve": "gatsby serve",
    "prettier": "prettier --write src/**/*.js",
    "lint": "eslint --debug src/",
    "lint:write": "eslint src/ --fix",
    "storybook": "start-storybook -p 9001",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx)": [
      "npm run format",
      "npm run lint:write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
